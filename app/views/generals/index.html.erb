<div id="imgIzq">
        
        <img src="img/logo.jpg" style="margin-left:17px; "/>
        <img src="img/o.png" style="margin-left:20px; margin-top:95%;"/>
       
	
    </div>

<div id="content" style="left:300px; background-color:#000; padding:10px; width:800px;">
          <h1 style=" color:#ffff00; ">Lista de Inscripciones Generales</h1>
          
          <table border=1 style="font-size:10pt;">
            <tr style="color:#ffff00; ">
              <th>Nombre Empresa</th>
              <th>Nombre Integrante 1</th>
              <th>Nombre Integrante 2</th>
              <th>Nombre Integrante 3</th>
              <th>Logotipo</th>
            </tr>
          
          <% @generals.each do |general| %>
            <tr style=" color:white; ">
              <td><%=h general.nombreEmpresa %></td>
              <td><%=h general.nombreIntegrante1 %></td>
              <td><%=h general.nombreIntegrante2 %></td>
              <td><%=h general.nombreIntegrante3 %></td>
              <td><%=h general.logotipo %></td>
              <td><%= link_to 'Mostrar', general %></td>
              <td><%= link_to 'Editar', edit_general_path(general) %></td>
              <td><%= link_to 'Eliminar', general, :confirm => 'Seguro?', :method => :delete %></td>
            </tr>
          <% end %>
          </table>
          
          <br />
          
</div>